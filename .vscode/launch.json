{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program":
        "${workspaceRoot}/src/iTextSharp.LGPLv2.Core.FunctionalTests/bin/Debug/net7.0/iTextSharp.LGPLv2.Core.FunctionalTests.dll",
      "args": [],
      "cwd": "${workspaceRoot}/src/iTextSharp.LGPLv2.Core.FunctionalTests",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
